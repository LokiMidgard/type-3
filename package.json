{
  "name": "excalibur-webpack",
  "version": "1.0.0",
  "private": true,
  "description": "a webpack starter for excaliburjs projects",
  "main": "index.js",
  "scripts": {
    "test": "npm run build:prod && node ./test/test.js",
    "webpack": "webpack",
    "dev": "webpack serve --config webpack.development.js",
